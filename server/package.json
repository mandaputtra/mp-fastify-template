    "bcrypt": "^3.0.5",
    "bcrypt-promise": "^2.0.0",
    "csrf": "^3.0.6",
    "dotenv": "^7.0.0",
    "fastify": "^2.1.0",
    "fastify-caching": "^5.0.0",
    "fastify-cookie": "^3.0.2",
    "fastify-cors": "^2.1.2",
    "fastify-helmet": "^3.0.0",
    "fastify-jwt": "^0.9.5",
